# SafeTransfer Secrets Template
# Rename this file to .env and fill in your keys

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Backend
FASTAPI_URL=http://localhost:8000

# Intelligence APIs
GOOGLE_API_KEY=your_gemini_api_key
PERPLEXITY_API_KEY=your_perplexity_api_key
CMC_API_KEY=your_coinmarketcap_api_key

# Exchange APIs (Read-Only Recommended)
BINANCE_API_KEY=your_binance_key
BINANCE_API_SECRET=your_binance_secret
BYBIT_API_KEY=your_bybit_key
BYBIT_API_SECRET=your_bybit_secret

# Database
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key

# Web3 RPCs (Optional - Defaults to Public Nodes)
RPC_ETH_MAINNET=https://eth.llamarpc.com
RPC_BSC_MAINNET=https://binance.llamarpc.com
RPC_POLYGON=https://polygon.llamarpc.com
RPC_ARBITRUM=https://arbitrum.llamarpc.com

# Security
SAFE_SENTINEL_API_KEY=your_secure_api_key_here
